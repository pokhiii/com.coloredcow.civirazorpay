<?xml version="1.0"?>
<extension key="com.coloredcow.civirazorpay" type="module">
  <file>civirazorpay</file>
  <name>Razorpay Payment Processor</name>
  <description>Accept payments using https://razorpay.com/</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>pokhiii</author>
    <email>pokhi@coloredcow.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">hhttps://lab.civicrm.org/extensions/civirazorpay</url>
    <url desc="Documentation">https://lab.civicrm.org/extensions/civirazorpay/-/blob/main/README.md</url>
    <url desc="Support">https://lab.civicrm.org/extensions/civirazorpay/-/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-11-01</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.77</ver>
  </compatibility>
  <comments>Original Author: Pokhi (ColoredCow)</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Civirazorpay</namespace>
    <format>23.02.1</format>
    <angularModule>crmCivirazorpay</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
</extension>
